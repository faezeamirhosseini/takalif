using System;

namespace tamrin1
{
    class program
    {
        public static void Main(string[] args) //hajm jabe
        {
            Console.WriteLine("Vared Konid 3 adad");

            int x = Convert.ToInt32(Console.ReadLine());
            int y = Convert.ToInt32(Console.ReadLine());
            int z = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine(calc(x,y,z));


        }
        private static int calc(int x, int y , int z)
        {
           int hajm = x * y * z;
            return hajm;
        }


    }
}
